<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lobsternetworks.android.fieldassistant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="7" />

        <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
        <uses-permission android:name="android.permission.INTERNET" />
   

    <application
        android:icon="@drawable/runner"
        android:label="@string/app_name" >
        <activity
            android:name="com.lobsternetworks.android.fieldassistant.main"
            android:label="@string/app_name" 
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Options"></activity>
        <activity android:name=".SelectEvent"></activity>
        <activity android:name=".Settings"></activity>
        <activity android:name=".FlightActivity"></activity>
        <activity android:name=".Input"></activity>
        <activity android:name=".Results"></activity>
        <activity android:name=".About"></activity>
        <activity android:name=".Preferences"></activity>
        <activity android:name=".Final"></activity>
    </application>

</manifest>